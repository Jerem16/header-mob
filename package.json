    "dependencies": {
        "next": "15.5.3",
        "react": "19.1.1",
        "react-dom": "19.1.1"
    },
    "devDependencies": {
        "@types/node": "^20",
        "@types/react": "19.1.12",
        "@types/react-dom": "19.1.9",
        "constructs": "^10.3.0",
        "esbuild": "^0.23.1",
        "eslint": "^9.15.0",
        "eslint-config-next": "15.5.3",
        "tsx": "^4.19.0",
        "typescript": "^5.6.2"
    },
    "packageManager": "yarn@4.9.2",
    "resolutions": {
        "@types/react": "19.1.12",
        "@types/react-dom": "19.1.9"
    }
